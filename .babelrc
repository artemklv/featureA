{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "transform-object-assign",
    "transform-function-bind",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "ignore": [
    "dist/**/*.js",
    "webpack.config.js",
    "webpack.config.dev.js",
    "bundle.js"
  ]
}
